#! python3
# script to print random anime "art" üëç
import random
import argparse

# {{{ parse args
parser = argparse.ArgumentParser(description='weeb faces üòà')
parser.add_argument('--neofetch',
                    help='enable colors for neofetch --ascii_colors argument. ex: neofetch --ascii "$(weeb --neofetch)" --ascii_colors 3',
                    dest='neofetch',
                    nargs='?',
                    required=False,
                    const=True,
                    default=False
                    )
args = parser.parse_args()
# }}}

